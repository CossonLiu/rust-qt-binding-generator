{
    "cppFile": "src/Processes.cpp",
    "rust": {
        "dir": "rust",
        "interfaceModule": "processes_interface",
        "implementationModule": "processes_implementation"
    },
    "objects": {
        "Processes": {
            "type": "UniformTree",
            "properties": {
                "active": {
                    "type": "bool",
                    "write": true
                }
            },
            "itemProperties": {
                "pid": {
                    "type": "quint32",
                    "roles": [ ["toolTip"] ]
                },
                "name": {
                    "type": "QString",
                    "roles": [ ["display"] ]
                },
                "cpuUsage": {
                    "type": "float",
                    "roles": [ [], ["display"] ]
                },
                "memory": {
                    "type": "quint64",
                    "roles": [ [], [], ["display"] ]
                },
                "uid": {
                    "type": "quint32"
                },
                "cpuPercentage": {
                    "type": "quint8"
                },
                "cmd": {
                    "type": "QString",
                    "rustByValue": true
                }
            }
        }
    }
}
